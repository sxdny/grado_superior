import java.util.Scanner;

public class ej4_24 {
    public static void main(String[] args) {
        
        int cdp = 0;
        Scanner sc = new Scanner(System.in);
        
        cdp = sc.nextInt();

        int[] array = new int[cdp];
        String[] resultado = new String[3];

        for (int k = 0; k < cdp; k++) {

            array[0] = sc.nextInt();
            array[1] = sc.nextInt();
            array[2] = sc.nextInt();
            
            int diff = array[0];
            do {
                diff++;
            } while (diff == array[1]);

            int diff2 = array[2];
            do {
                diff2--;
            } while (diff2 == array[1]);

            if (diff > diff2) {
                resultado[k] = Integer.toString(array[1]);
            }
            else if(diff < diff2) {
                resultado[k] = Integer.toString(array[2]);
            }
            else {
                resultado[k] = "EMPATE";
            }

        }

        System.out.print("\n");

        for (int i = 0; i < resultado.length; i++) {
            System.out.println(resultado[i]);
        }

    }
}

    
